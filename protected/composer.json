{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "barryvdh/laravel-dompdf": "^0.7.0",
        "barryvdh/laravel-snappy": "^0.3.2",
        "garygreen/pretty-routes": "^0.0.5",       
        "laravelcollective/html": "^5.3.0",       
        "zizaco/entrust": "5.2.x-dev",        
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.5",
        "jeremykenedy/laravel-roles": "^3.0",
        "laravel/framework": "^6.2",
        "laravel/passport": "^8.4",
        "laravel/tinker": "^2.0",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "doctrine/dbal": "v2.5.5",
        "facade/ignition": "^1.4",       
        "laravel/ui": "^1.2",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "ottaviano/faker-gravatar": "^0.1.2",
        "phpunit/phpunit": "^8.0"		
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Http/helpers.php",
            "app/Http/cashReportHelper.php",
            "app/Http/inventoryReportHelper.php",
            "app/Http/SystemAuditHelper.php",
            "app/Http/AssessmentsHelper.php",
            "app/Http/ReferralsHelper.php",
            "app/Http/CaseReportHelper.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
